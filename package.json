{
  "name": "nestjs-crypto",
  "version": "1.0.1",
  "description": "Crypto module for NestJS",
  "keywords": [
    "nestjs",
    "crypto",
    "encryption",
    "decryption",
    "hashing",
    "security",
    "nestjs-module",
    "bcrypt",
    "aes",
    "aes-256-cbc",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "cstrp <cstrp@pm.me>",
  "license": "MIT",
  "type": "commonjs",
  "homepage": "https://cstrp.github.io/nestjs-crypto/",
  "repository": {
    "type": "git",
    "url": "https://github.com/cstrp/nestjs-crypto.git"
  },
  "bugs": {
    "url": "https://github.com/cstrp/nestjs-crypto/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "build:check": "tsc --noEmit",
    "clean": "rm -rf dist coverage .vitepress/cache .vitepress/dist node_modules/.cache",
    "lint": "eslint lib/**/*.ts",
    "lint:fix": "eslint lib/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "publish:npm": "npm publish --access public",
    "version:patch": "npm version patch -m 'chore: bump version to %s'",
    "version:minor": "npm version minor -m 'chore: bump version to %s'",
    "version:major": "npm version major -m 'chore: bump version to %s'",
    "release:patch": "npm run version:patch && git push && git push --tags",
    "release:minor": "npm run version:minor && git push && git push --tags",
    "release:major": "npm run version:major && git push && git push --tags",
    "typedoc:build": "typedoc --tsconfig ./tsconfig.typedoc.json",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "benchmark": "ts-node lib/utils/benchmark.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-angular": "20.0.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "24.8.1",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "bcrypt": "^6.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "gh-pages": "^6.3.0",
    "husky": "9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "16.2.6",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "prettier": "3.6.2",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typedoc": "0.28.14",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.0",
    "vitepress": "^1.6.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "bcrypt": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "typescript": "^4.1.2 || ^5.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}
